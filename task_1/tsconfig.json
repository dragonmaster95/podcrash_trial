{
	"compilerOptions": {
		"target": "es6",
		"moduleResolution": "Node10",
		"module": "ES2020",
		"declaration": false,
		"noLib": false,
		"sourceMap": true,
		"pretty": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"allowUnreachableCode": true,
		"allowUnusedLabels": true,
		"noImplicitAny": true,
		"noImplicitReturns": false,
		"noImplicitUseStrict": false,
		"outDir": "build/BP",
		"rootDir": ".",
		"baseUrl": ".",
		"listFiles": false,
		"noEmitHelpers": false,
		"skipLibCheck": true
	},
	"include": ["scripts/**/*"],
	"exclude": ["lib", "dist", "node_modules"],
	"compileOnSave": false
}
